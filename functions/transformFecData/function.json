{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "inputblob",
      "direction": "in",
      "type": "blob",
      "path": "control-blobs/etl-control.json",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "path": "final-data/final2020.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
